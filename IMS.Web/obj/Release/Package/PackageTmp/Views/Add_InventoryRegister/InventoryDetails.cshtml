@model IEnumerable<IMS.Models.IMSEntity>

@{
    string role = ViewBag.role;
    string empid = ViewBag.empid;
    string dept = ViewBag.department;
    string location = ViewBag.location;
    string inwardID = ViewBag.inwardID;
    string expense = ViewBag.expensenature;

    ViewBag.Title = "InventoryDetails";
    Layout = "~/Views/Shared/_Layout_IMS.cshtml";
}


<link href="~/assets/css/custom.css" rel="stylesheet" />


<div class="container-fluid">
    <div class="card" style="margin-left:-50px;">
        <div class="card-body">         
            <div class="row">
                <div class="col-md-12">
                    <div class="card-header">
                       Inventory Register Details
                    </div>
                    <div class="card-body">
                        <table>
                            <thead>
                                <tr>
                                    <th class="col-lg-2">Asset ID</th>
                                    <th class="col-lg-2">MaterialCategory Name</th>
                                    <th class="col-lg-2">MaterialName</th>
                                    <th class="col-lg-2">Expense Nature</th>
                                    <th class="col-lg-2">GRN Number</th>
                                </tr>
                            </thead>
                            <tr>
                                @foreach (var item in Model)
                                {
                                    <td>
                                        @Html.DisplayFor(modelitem => item.AssetID)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Material_CategoryName)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelitem => item.MaterialName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Inward_ExpenseNature)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.GRN_Number)
                                    </td>
                                }
                            </tr>
                        </table>

                        <hr />
                        <table>
                            <thead>
                                <tr>
                                    <th class="col-lg-2">Department Name</th>
                                    <th class="col-lg-2">Purchase Rate</th>
                                    <th class="col-lg-2">Asset Cost</th>
                                    <th class="col-lg-2">Make</th>
                                    <th class="col-lg-2">Serial No</th>
                                    <th class="col-lg-2">Model No</th>
                                </tr>
                            </thead>
                            <tr>
                                @foreach (var item in Model)
                                {
                                    <td>

                                        @Html.DisplayFor(modelitem => item.userDepartmentName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Purchase_Rate)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelitem => item.Asset_Cost)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Makenm)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Serial_No)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelitem => item.Model_Number)
                                    </td>
                                }
                            </tr>
                        </table>

                     
                    </div>

                </div>
            </div>
        </div>
    </div>

   
</div>
<script src="../assets/libs/jquery/dist/jquery.min.js"></script>
